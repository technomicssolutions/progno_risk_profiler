	  <%= form_tag({:controller => 'advisor', :action => 'financial_view'}) do  %>
		  <table class="table table-striped table-condensed table-hover">
		    <% @total.each do |f| %>
		    	<tr>
			      <th><%= f.question.html_safe %></th>
		      	<th><%= text_field_tag(:"#{f.map_variable_id}") %></th>
		    	</tr>
	    	<% end %>
	    </table>
	    <div>
	    	<%= submit_tag "Submit", class:'btn btn-success'%>
	    </div>
	  <% end %>
	
