<%= form_for(@user, :url => user_registration_path) do |f| %>
<%= f.label :email, "Email" %>
<%= f.email_field :email %>

<%= f.label :password, "Password" %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, "Password _Confirmation" %>
<%= f.password_field :password_confirmation %>

<%= f.submit "Sing Up"%>
  
<% end -%>